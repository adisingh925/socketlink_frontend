{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/login",
        "destination": "/login.html"
      },
      {
        "source": "/signup",
        "destination": "/signup.html"
      },
      {
        "source": "/auth/action",
        "destination": "/auth/action.html"
      },
      {
        "source": "/forgot-password",
        "destination": "/forgot-password.html"
      },
      {
        "source": "/profile",
        "destination": "/profile.html"
      },
      {
        "source": "/contact-us",
        "destination": "/contact-us.html"
      },
      {
        "source": "/metrics",
        "destination": "/metrics.html"
      },
      {
        "source": "/my-plans",
        "destination": "/my-plans.html"
      },
      {
        "source": "/pricing",
        "destination": "/pricing.html"
      },
      {
        "source": "/privacy-policy",
        "destination": "/privacy-policy.html"
      },
      {
        "source": "/terms-conditions",
        "destination": "/terms-conditions.html"
      },
      {
        "source": "/billing",
        "destination": "/billing.html"
      },
      {
        "source": "/configuration",
        "destination": "/configuration.html"
      },
      {
        "source": "/docs",
        "destination": "/docs.html"
      },
      {
        "source": "/docs/introduction",
        "destination": "/docs/introduction.html"
      },
      {
        "source": "/docs/admin-endpoints",
        "destination": "/docs/admin-endpoints.html"
      },
      {
        "source": "/docs/client-endpoints",
        "destination": "/docs/client-endpoints.html"
      },
      {
        "source": "docs/third-party-libraries",
        "destination": "docs/third-party-libraries.html"
      },
      {
        "source": "/docs/features",
        "destination": "/docs/features.html"
      },
      {
        "source": "/docs/payment-options",
        "destination": "/docs/payment-options.html"
      },
      {
        "source": "/docs/refund-policy",
        "destination": "/docs/refund-policy.html"
      },
      {
        "source": "/docs/all-events",
        "destination": "/docs/all-events.html"
      },
      {
        "source": "/docs/sending-messages",
        "destination": "/docs/sending-messages.html"
      },
      {
        "source": "/docs/mysql-integration",
        "destination": "/docs/mysql-integration.html"
      },
      {
        "source": "/docs/webhooks",
        "destination": "/docs/webhooks.html"
      },
      {
        "source": "/docs/how-to-purchase",
        "destination": "/docs/how-to-purchase.html"
      },
      {
        "source": "/docs/room-types",
        "destination": "/docs/room-types.html"
      },
      {
        "source": "docs/javascript-quick-start",
        "destination": "docs/javascript-quick-start.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; includeSubDomains; preload"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "Referrer-Policy",
            "value": "strict-origin-when-cross-origin"
          },
          {
            "key": "Permissions-Policy",
            "value": "geolocation=(self), microphone=(self), camera=(self)"
          },
          {
            "key": "X-Permitted-Cross-Domain-Policies",
            "value": "none"
          },
          {
            "key": "X-XSS-Protection",
            "value": "0"
          },
          {
            "key": "Expect-CT",
            "value": "max-age=86400, enforce"
          },
          {
            "key": "X-Robots-Tag",
            "value": "index, follow"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Security-Policy",
            "value": "default-src * 'unsafe-inline' 'unsafe-eval' data: blob:;"
          }                                                                                                               
        ]
      }
    ]
  }
}